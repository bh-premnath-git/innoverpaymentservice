{
  "name": "WSO2-IS",
  "displayName": "WSO2 Identity Server 7",
  "type": "WSO2-IS",
  "description": "WSO2 Identity Server 7.1.0 as Third-Party Key Manager for APIM 4.5.0",
  "enabled": true,
  "issuer": "https://wso2is:9443/oauth2/token",
  "availableGrantTypes": [
    "password",
    "client_credentials",
    "authorization_code",
    "refresh_token",
    "urn:ietf:params:oauth:grant-type:saml2-bearer",
    "urn:ietf:params:oauth:grant-type:jwt-bearer"
  ],
  "enableTokenGeneration": true,
  "enableTokenEncryption": false,
  "enableTokenHashing": false,
  "enableOAuthAppCreation": true,
  "enableMapOAuthConsumerApps": false,
  "enableSelfValidationJWT": true,
  "claimMapping": [
    {
      "remoteClaim": "sub",
      "localClaim": "http://wso2.org/claims/username"
    },
    {
      "remoteClaim": "email",
      "localClaim": "http://wso2.org/claims/emailaddress"
    },
    {
      "remoteClaim": "groups",
      "localClaim": "http://wso2.org/claims/role"
    }
  ],
  "endpoints": {
    "authorize": "https://wso2is:9443/oauth2/authorize",
    "token": "https://wso2is:9443/oauth2/token",
    "revoke": "https://wso2is:9443/oauth2/revoke",
    "userinfo": "https://wso2is:9443/scim2/Me",
    "introspection": "https://wso2is:9443/oauth2/introspect",
    "jwks": "https://wso2is:9443/oauth2/jwks",
    "client_registration": "https://wso2is:9443/api/identity/oauth2/dcr/v1.1/register",
    "well_known": "https://wso2is:9443/oauth2/token/.well-known/openid-configuration"
  },
  "certificates": {
    "type": "JWKS",
    "value": "https://wso2is:9443/oauth2/jwks"
  },
  "alias": [],
  "additionalProperties": {
    "Username": "admin",
    "Password": "admin",
    "self_validate_jwt": "true",
    "claim_mappings": []
  }
}
